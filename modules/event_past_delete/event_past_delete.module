<?php

function event_past_delete_cron() {
  $queue_event = \Drupal::queue('event_past_delete_CronQueue');
  $data = [ 'entity_type' => 'node', 'id' => 1 ]; 
  $queue_event->createItem($data);

  $queue_file = \Drupal::queue('event_past_delete_FileCronQueue');
  $queue_file->createItem($data);
}

